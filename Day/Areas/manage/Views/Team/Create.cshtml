@model TeamMember
@{
    ViewData["Title"] = "Create";
}

<form asp-action="create" enctype="multipart/form-data">
    <div class="mb-3">
        <label asp-for="FullName" class="form-label">FullName</label>
        <input asp-for="FullName" class="form-control">
        <span asp-validation-for="FullName" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="Position" class="form-label">Position</label>
        <input asp-for="Position" class="form-control">
        <span asp-validation-for="Position" class="text-danger"></span>

    </div>
    <div class="mb-3">
        <label asp-for="Desc" class="form-label">Desc</label>
        <input asp-for="Desc" class="form-control">
        <span asp-validation-for="Desc" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="TwitterUrl" class="form-label">TwitterUrl</label>
        <input asp-for="TwitterUrl" class="form-control">
        <span asp-validation-for="TwitterUrl" class="text-danger"></span>

    </div>
    <div class="mb-3">
        <label asp-for="InstaUrl" class="form-label">InstaUrl</label>
        <input asp-for="InstaUrl" class="form-control">
        <span asp-validation-for="InstaUrl" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="LinkednUrl" class="form-label">LinkednUrl</label>
        <input asp-for="LinkednUrl" class="form-control">
        <span asp-validation-for="LinkednUrl" class="text-danger"></span>

        <div class="mb-3">
            <label asp-for="FacebookUrl" class="form-label">FacebookUrl</label>
            <input asp-for="FacebookUrl" class="form-control">
            <span asp-validation-for="FacebookUrl" class="text-danger"></span>
        </div>
        <div class="mb-3">
            
            <img />

        </div>
    </div>
   
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
